PROJECT_NAME = RoxeeTorrent
PROJECT_VERSION_MAJOR = {PUKE-VERSION-MAJOR}
PROJECT_VERSION_MINOR = {PUKE-VERSION-MINOR}
PROJECT_VERSION_BUGFIX = {PUKE-VERSION-BUGFIX}
PROJECT_OUTPUT_DIR = {PUKE-OUTPUT-DIR}

isEmpty(PROJECT_OUTPUT_DIR){
    PROJECT_OUTPUT_DIR = $$PWD/buildd/$$PROJECT_NAME-$$PROJECT_VERSION_MAJOR.PROJECT_VERSION_MINOR.PROJECT_VERSION_BUGFIX-release/$$GITVERSION-$$GITCHANGENUMBER
}

OTHER_FILES += vars.pri.in
